{
  "name": "@oigroup/babel-plugin-lightscript",
  "version": "1.0.3",
  "description": "Compile LightScript to JavaScript.",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "npm run build && npm run test:only",
    "test:only": "mocha test",
    "test:debug": "node --inspect --debug-brk node_modules/.bin/_mocha test",
    "build": "babel src --out-dir .",
    "prepublish": "npm run build",
    "diag": "./bin/lscdiag.js"
  },
  "bin": {
    "lscdiag": "./bin/lscdiag.js"
  },
  "author": "Alex Rattray <rattray.alex@gmail.com> (http://alexrattray.com/)",
  "homepage": "http://lightscript.org/",
  "license": "MIT",
  "dependencies": {
    "@oigroup/babylon-lightscript": "^1.0.3",
    "ast-loc-utils": "^1.0.3",
    "@oigroup/lightscript-ast-transforms": "1.0.4"
  },
  "files": [
    "bin",
    "stdlib.js",
    "index.js"
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-helper-plugin-test-runner": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.3.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-babel": "^6.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "mocha": "^3.2.0",
    "glob": "^7.1.1",
    "yargs": "^7.0.2"
  }
}
